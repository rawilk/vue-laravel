{
  "name": "lara-vue",
  "version": "0.1.0",
  "description": "A JavaScript framework built for Laravel with VueJS",
  "keywords": [
    "Vue",
    "Vue2",
    "Laravel",
    "Admin",
    "Bootstrap",
    "Bootstrap 4"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --env.mixfile=build/webpack.mix.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --env.mixfile=build/webpack.mix.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --env.mixfile=build/webpack.mix.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "author": "Randall Wilk",
  "copyright": "Copyright 2018 Randall Wilk",
  "license": "MIT",
  "main": "dist/js/lara-vue.js",
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.1.6",
    "laravel-mix": "^2.1.11",
    "node-sass": "^4.9.0"
  },
  "dependencies": {
    "@mdi/font": "^2.3.54",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.startcase": "^4.4.0",
    "popper.js": "^1.14.3",
    "qs": "^6.5.2",
    "recursive-iterator": "^3.3.0",
    "vue": "^2.5.16",
    "vue-context": "^3.0.1",
    "vue-functional-data-merge": "^2.0.6"
  }
